{
  "name": "i-seek-you",
  "version": "0.1.0",
  "description": "A WebRTC-based cross-platform data transfer application.",
  "private": "true",
  "scripts": {
    "start": "vite",
    "dev": "vite --debug",
    "preview": "vite build && vite preview",
    "build": "vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "cz": "cz",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "cloc": "cloc src --exclude-ext=svg",
    "posttest": "npm run format",
    "check": "npx eslint .",
    "test": "playwright test"
  },
  "author": {
    "name": "Lifeni",
    "email": "liangfengning@foxmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lifeni/i-seek-you.git"
  },
  "homepage": "https://github.com/Lifeni/i-seek-you#readme",
  "bugs": {
    "url": "https://github.com/Lifeni/i-seek-you/issues",
    "email": "liangfengning@foxmail.com"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.1",
    "@types/lodash": "^4.14.181",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@windicss/plugin-animations": "^1.0.9",
    "@windicss/plugin-scrollbar": "^1.2.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-solid": "^0.4.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-solid": "^2.2.6",
    "vite-plugin-windicss": "^1.8.4",
    "windicss": "^3.5.1"
  },
  "dependencies": {
    "@lifeni/libsm-js": "0.1.4",
    "@nuintun/qrcode": "^3.1.1",
    "@ultimate/vault": "^0.0.2",
    "gemoji": "^7.1.0",
    "lodash": "^4.17.21",
    "solid-app-router": "^0.3.2",
    "solid-icons": "^0.3.3",
    "solid-js": "^1.3.15",
    "solid-meta": "^0.27.3",
    "tinykeys": "^1.4.0",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx,css,md}": "prettier --write",
    "*.{ts,tsx}": "eslint ."
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
