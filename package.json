{
  "name": "i-seek-you",
  "version": "0.1.0",
  "description": "A WebRTC-based cross-platform data transfer application.",
  "private": "true",
  "scripts": {
    "start": "vite --host --port=8080",
    "dev": "vite --host --port=8080",
    "build": "vite build",
    "serve": "vite preview --host --port=8080",
    "prepare": "husky install",
    "cz": "cz",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format"
  },
  "author": {
    "name": "Lifeni",
    "email": "liangfengning@foxmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lifeni/i-seek-you.git"
  },
  "homepage": "https://github.com/Lifeni/i-seek-you#readme",
  "bugs": {
    "url": "https://github.com/Lifeni/i-seek-you/issues",
    "email": "liangfengning@foxmail.com"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.1.1",
    "@unocss/preset-attributify": "^0.27.2",
    "@unocss/preset-icons": "^0.27.2",
    "@unocss/preset-uno": "^0.27.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "unocss": "^0.27.2",
    "vite": "^2.8.6",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "solid-app-router": "^0.3.0",
    "solid-js": "^1.3.9",
    "solid-meta": "^0.27.3"
  },
  "lint-staged": {
    "*.{ts,tsx,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
