{
  "name": "i-seek-you",
  "version": "0.0.0",
  "description": "A WebRTC-based cross-platform data transfer application.",
  "scripts": {
    "start": "vite --host --port=8080",
    "dev": "vite --host --port=8080",
    "build": "vite build",
    "serve": "vite preview --host --port=8080",
    "prepare": "husky install",
    "cz": "cz",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format"
  },
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "solid-js": "^1.3.9"
  },
  "lint-staged": {
    "*.{ts,tsx,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
